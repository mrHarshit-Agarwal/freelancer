<script>
  $(function(){
      $('#eye').click(function(){
          if($(this).hasClass('flaticon-view')){
              $(this).removeClass('flaticon-view');
              $(this).addClass('flaticon-hide');
              $('#loginpassword').attr('type','text');
          }else{
              $(this).removeClass('flaticon-hide');
              $(this).addClass('flaticon-view');  
              $('#loginpassword').attr('type','password');
          }
      });
  });
</script>
